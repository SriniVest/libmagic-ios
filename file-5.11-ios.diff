--- file-5.11.orig/Makefile.am	2010-07-22 01:17:41.000000000 +0900
+++ file-5.11/Makefile.am	2012-03-10 01:01:57.000000000 +0900
@@ -3,3 +3,22 @@ ACLOCAL_AMFLAGS = -I m4
 EXTRA_DIST = MAINT
 
 SUBDIRS = src magic tests doc python
+
+IOS_BASEDIR = $(top_srcdir)/ios
+IOS_SRCDIR = $(IOS_BASEDIR)/magic
+IOS_SRC_FILES = apprentice.c apptype.c ascmagic.c cdf.c cdf.h cdf_time.c \
+		compress.c elfclass.h encoding.c file.h \
+		fsmagic.c funcs.c is_tar.c magic.c magic.h print.c \
+		readcdf.c readelf.c readelf.h softmagic.c tar.h
+
+.PHONY: ios
+
+ios: all
+	cp -p config.h $(IOS_SRCDIR)
+	(cd $(top_srcdir)/magic; $(MAKE) display-magic) > $(IOS_SRCDIR)/magic
+	for f in $(IOS_SRC_FILES); do \
+		cp -p $(top_srcdir)/src/$$f $(IOS_SRCDIR)/$$f; \
+	done
+
+clean-local:
+	cd $(IOS_SRCDIR); rm -f config.h magic $(IOS_SRC_FILES)
